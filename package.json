{
  "name": "prototype-node",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "clear": "rm -rf dist coverage",
    "dev": "nodemon --delay 3 --watch src --ext ts --exec \"run-s build-only start\"",
    "build": "run-s clear build-only",
    "build-only": "tsc --build --force tsconfig.app.json && tsc-alias -p tsconfig.app.json",
    "start": "node dist/index.js",
    "format": "eslint . --fix",
    "postinstall": "pnpm run codegen",
    "codegen": "pnpm run \"/^codegen:.*/\" && tsc -p tsconfig.codegen.json",
    "codegen:graphql-example": "graphql-codegen --config .graphqlrc.yaml -p example",
    "codegen:openapi-example": "openapi-ts -f openapi.config.ts  -i ./example.openapi.yaml -o ./src/generated/openapi-example",
    "check": "pnpm run \"/^check:.*/\"",
    "check:lint": "eslint .",
    "check:tsc": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --testMatch '**/*.test.ts' --passWithNoTests",
    "test:integration": "jest --testMatch '**/*.spec.ts' --passWithNoTests",
    "test:watch": "jest --watchAll",
    "prepare": "ts-patch install && typia patch",
    "upgrade-packages": "ncu --doctor -u --doctorInstall \"pnpm install\" --doctorTest \"run-p check test\" --peer --target semver",
    "validate-runtime": "tsx validate-runtime.script.ts",
    "upgrade-packages-latest": "ncu --doctor -u --doctorInstall \"pnpm install\" --doctorTest \"run-p check test validate-runtime\" --peer --target latest"
  },
  "author": "Junchi Wang <benson26365156@gmail.com> (https://chi0307.github.io)",
  "license": "ISC",
  "description": "A prototype Node.js application with modern development tooling.",
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.25.1",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typed-document-node": "^5.1.1",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@hey-api/openapi-ts": "^0.66.5",
    "@tsconfig/node22": "^22.0.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import-x": "^4.10.6",
    "eslint-plugin-perfectionist": "^4.12.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "npm-check-updates": "^18.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-patch": "^3.3.0",
    "tsc-alias": "1.8.13",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  },
  "dependencies": {
    "@chi0307/axios-wrapper": "^2.0.0",
    "@hey-api/client-fetch": "^0.10.0",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "graphql-request": "^7.1.2",
    "typia": "^9.1.1",
    "zod": "^3.24.3"
  },
  "keywords": [
    "Node.js",
    "TypeScript",
    "typia",
    "prototype",
    "axios"
  ]
}
