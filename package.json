{
  "name": "prototype-node",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "clear": "rm -rf dist coverage typia-generated",
    "dev:typia": "typia generate --project tsconfig.app.json --input src --output typia-generated",
    "dev:start": "tsx --tsconfig ./tsconfig.app.json typia-generated/index.ts",
    "dev": "nodemon --watch src --ext ts --exec \"pnpm run dev:typia && pnpm run dev:start\"",
    "build": "pnpm run clear && pnpm run build-only",
    "build-only": "tsc --build --force tsconfig.app.json && tsc-alias -p tsconfig.app.json",
    "start": "node dist/index.js",
    "format": "eslint . --fix",
    "check": "pnpm run \"/^check:.*/\"",
    "check:lint": "eslint .",
    "check:tsc": "tsc --noEmit",
    "test": "pnpm run \"/^test:.*/\"",
    "test:unit": "jest",
    "prepare": "ts-patch install && typia patch"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@tsconfig/node22": "^22.0.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.6",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-perfectionist": "^4.6.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-patch": "^3.3.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "typia": "^7.6.0"
  }
}
